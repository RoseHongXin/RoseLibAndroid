<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/_line</item>
    </style>
    <style name="vertical_line">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/_line</item>
    </style>

    <style name="RoseDlgAlert" parent="Theme.AppCompat.Dialog.Alert">
        <!--android:windowTitleStyle 和title有關, windowBackground和部分自定義style有關聯-->
        <!--windowBackground, colorButtonNormal, colorPressedHighlight和部分自定義style有關聯-->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/windowBackground</item>
        <item name="android:textColor">@color/textColorPrimary</item>
        <item name="android:textColorSecondary">@color/textColorPrimary</item>
        <item name="textColorAlertDialogListItem">@color/textColorPrimary</item>
    </style>

